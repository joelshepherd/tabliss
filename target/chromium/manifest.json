{
  "manifest_version": 3,
  "name": "Tab Nine",
  "short_name": "Tab Nine",
  "version": "1.1.0",
  "description": "A modern, highly customisable New Tab page with many widgets that does not require any permissions.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "32": "icons/32.png",
    "48": "icons/48.png",
    "96": "icons/96.png",
    "128": "icons/128.png"
  },
  "permissions": ["storage"]
}
